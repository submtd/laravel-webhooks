{
    "name": "submtd/laravel-webhooks",
    "description": "Send webhooks from a Laravel application",
    "authors": [
        {
            "name": "Steve Harmeyer",
            "email": "harmeyersteve@gmail.com"
        }
    ],
    "require": {
        "submtd/laravel-http-request": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Submtd\\LaravelWebhooks\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Submtd\\LaravelWebhooks\\Providers\\LaravelWebhooksServiceProvider"
            ],
            "aliases": {
                "LaravelWebhooks": "Submtd\\LaravelWebhooks\\Facades\\LaravelWebhooksFacade"
            }
        }
    }
}