{
    "name": "submtd/laravel-webhooks",
    "description": "Send webhooks from a Laravel application",
    "authors": [
        {
            "name": "Steve Harmeyer",
            "email": "harmeyersteve@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "submtd/laravel-request-scope": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Submtd\\LaravelWebhooks\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Submtd\\LaravelWebhooks\\Providers\\WebhooksServiceProvider"
            ],
            "aliases": {
                "Webhooks": "Submtd\\LaravelWebhooks\\Facades\\Webhooks"
            }
        }
    }
}